{
  "name": "berlioz/hector-package",
  "type": "berlioz-package",
  "description": "Hector ORM package for Berlioz Framework",
  "minimum-stability": "beta",
  "prefer-stable": true,
  "license": "MIT",
  "homepage": "https://getberlioz.com",
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan@getberlioz.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\Package\\Hector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\Package\\Hector\\Tests\\": "tests/",
      "Berlioz\\Package\\Hector\\TestProject\\": "tests_env/src/"
    },
    "exclude-from-classmap": [
      "/tests_env/src/"
    ]
  },
  "require": {
    "php": "^8.0",
    "berlioz/core": "^2.0",
    "hectororm/orm": "^1.0",
    "doctrine/sql-formatter": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "berlioz/http-core": "^2.0"
  },
  "config": {
    "berlioz": {
      "package": "Berlioz\\Package\\Hector\\BerliozPackage"
    }
  }
}
